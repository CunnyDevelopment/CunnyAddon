{
  "schemaVersion": 1,
  "id": "cunny-addon",
  "version": "${version}",
  "name": "Cunny Addon",
  "description": "A meteor addon made by Neko",
  "authors": [
    "CunnyDevelopment"
  ],
  "contact": {
    "repo": "https://github.com/CunnyDevelopment/CunnyAddon"
  },
  "icon": "assets/template/icon.png",
  "environment": "client",
  "entrypoints": {
    "meteor": [
      "io.github.cunnydevelopment.cunnyaddon.Cunny"
    ]
  },
  "mixins": [
    "cunny.mixins.json",
    "cunny_meteorclient.mixins.json"
  ],
  "accessWidener": "cunny.accesswidener",
  "custom": {
    "meteor-client:color": "0,255,234"
  },
  "depends": {
    "java": ">=17",
    "minecraft": ">=${mc_version}",
    "meteor-client": "*"
  }
}
